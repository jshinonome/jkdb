{
  "name": "jkdb",
  "version": "1.1.5",
  "description": "javascript interface for kdb+/q",
  "keywords": [
    "q",
    "kx",
    "kdb+"
  ],
  "main": "jkdb.min.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --testPathPattern=ipc",
    "integration": "jest --testPathPattern=integration",
