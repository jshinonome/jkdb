{
  "name": "jkdb",
  "version": "1.2.1",
  "description": "javascript interface for kdb+/q",
  "keywords": [
    "q",
    "kx",
    "kdb+"
  ],
  "main": "jkdb.min.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --testPathPatterns=ipc",
    "integration": "jest --testPathPatterns=integration",
