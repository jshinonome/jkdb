  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-object-rest-spread": "7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/node": "^25.2.2",
    "babel-jest": "^30.2.0",
    "eslint": "^10.0.0",
    "eslint-plugin-jest": "^29.12.2",
    "esm": "^3.2.25",
    "jest": "^30.2.0",
    "rollup": "^4.57.1",
    "typescript": "^5.9.3",
    "@rollup/plugin-terser": "^0.4.4"
